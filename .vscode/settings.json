{
  "python.formatting.provider": "black",
  "Codegeex.RepoIndex": true,
  "files.associations": {
    "*.vue": "html",
    "*.cjson": "jsonc",
    "*.wxs": "javascript",
    "*.wxml": "html",
    "*.wxss": "css",
    "iostream": "cpp",
    "atomic": "cpp",
    "*.tcc": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "initializer_list": "cpp",
    "iosfwd": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "typeinfo": "cpp",
    "array": "cpp",
    "deque": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp",
    "optional": "cpp",
    "fstream": "cpp",
    "sstream": "cpp"
  },
  "code-runner.executorMap": {
    "cpp": "cd /d \"$dir\" && (if not exist output mkdir output) && g++ \"$fileName\" -o \"output\\$fileNameWithoutExt.exe\" && call \"output\\$fileNameWithoutExt.exe\""
  },
  "C_Cpp.debuggerPath": "D:\\mingw64\\mingw64\\bin\\gdb.exe", // 替换为你的实际路径
  "cmake.debugConfig": {
    "miDebuggerPath": "D:\\mingw64\\mingw64\\bin\\gdb.exe"
  },
  "terminal.external.windowsExec": "cmd.exe", // 强制使用CMD
  // "debug.allowBreakpointsEverywhere": true,
  "code-runner.runInTerminal": true,
  // "debug.javascript.autoAttachFilter": "onlyWithFlag",
  // "debug.toolBarLocation": "docked",
  // "debug.console.closeOnEnd": true
  "debug.allowBreakpointsEverywhere": false,
  "debug.javascript.autoAttachFilter": "onlyWithFlag",
  "debug.showInStatusBar": "always",
  "cmake.sourceDirectory": "E:/GitHub/my_item/.vscode"
}
